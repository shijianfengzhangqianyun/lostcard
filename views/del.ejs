<% include header %>
    <h1>删除学生信息</h1>
    <form method="post">
        <ul>
            <% for(var i = 0; i < result.length; i++) { %>
            <li>
                <label for="student">学工号：<%= result[i].studentId %>，领取地址：<%= result[i].name %>，发布时间：<%= result[i].sdate%></label>
                <input type="radio" name="student" value=<%= result[i]._id%>>
            </li>
            <% } %>
        </ul>
        <button type="submit">确定</button>
    </form>
<% include footer %>